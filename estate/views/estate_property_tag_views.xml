<?xml version="1.0"?>
<odoo>
    <record id="estate_model_tag_action" model="ir.actions.act_window">
        <field name="name">Estate tags</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            Estate tags
          </p>
        </field>
    </record>
    <record id="estate_view_tag_tree" model="ir.ui.view">
        <field name="name">estate.property.tag.tree</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <tree string="tags list" editable="bottom">
                <field name="name" string="Title"/>
            </tree>
        </field>
    </record>
        <record id="estate_view_tag_form" model="ir.ui.view">
        <field name="name">estate.property.tag.form</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <form string="form test">
                <sheet>
                    <div class="oe_title"><h1><field name="name" string="Title"/></h1></div>
                </sheet>
            </form>
        </field>
    </record>
        <record id="estate_view_tag_search" model="ir.ui.view">
        <field name="name">estate.property.tag.search</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <search string="Search Estate">
                <field name="name" string="Title"/>
            </search>
        </field>
    </record>
</odoo>